{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "motor-cms/motor-backend": "^0.6",
        "motor-cms/motor-cms": "^0.6",
        "motor-cms/motor-core": "^0.6",
        "motor-cms/motor-media": "^0.6",
        "partymeister/accounting": "^0.3",
        "partymeister/competitions": "^0.3",
        "partymeister/core": "^0.3",
        "partymeister/frontend": "^0.3",
        "partymeister/slides": "^0.3"
    },
    "require-dev": {
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan motor:setpackagedev false"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan motor:setpackagedev false"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
